<h2>
  Welcome, <%= @user.username %>!
</h2>

<h3 class="mb-none">
  Diet Record
</h3>
<ul id="users-products-list" >
  <% if @user.users_products %>
    <% @user.users_products.each do |users_product| %>
      <li id="users-product-<%= users_product.id %>-container">
        <%= render partial: "users_products/users_product", layout: false, locals: {users_product: users_product} %>
      </li>
    <% end %>
  <% else %>
    <p>
      Check out the home page to add some food records
    </p>
  <% end %>
</ul>
