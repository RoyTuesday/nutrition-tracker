<h2><%= @user.username %></h2>

<ul>
  <% if @user.users_products %>
    <% @user.users_products.each do |users_product| %>
      <% product = users_product.product %>
      <%= render partial: "users_products/users_product", layout: false, locals: {users_product: users_product, product: product} %>
    <% end %>
  <% else %>
    <p>
      Check out the home page to add some food records
    </p>
  <% end %>
</ul>
